{
  "name": "sentence-collector-web",
  "version": "0.0.1",
  "description": "A web console for collecting and validating sentences",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start": "./node_modules/webpack/bin/webpack.js --watch --mode development",
    "build": "./node_modules/webpack/bin/webpack.js --mode production",
    "lint": "eslint src",
    "test": "npm run lint && jest",
    "test:unit": "jest"
  },
  "dependencies": {
    "connected-react-router": "^6.9.1",
    "hammerjs": "^2.0.8",
    "iso-639-1": "^2.1.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/runtime": "^7.13.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.2",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.23",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/regenerator-runtime": "^0.13.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.1",
    "eslint": "^7.24.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.23.2",
    "file-loader": "^6.2.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "mini-css-extract-plugin": "^1.4.1",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/node_modules/**",
      "!**/tests/**",
      "!**/src/components/swipecard/**",
      "!webpack.config.js"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 82,
        "lines": 93,
        "statements": 91
      }
    },
    "preset": "ts-jest",
    "transform": {
      "^.+\\.js?$": "babel-jest",
      "^.+\\.ts?$": "ts-jest",
      "\\.svg$|\\.css$": "<rootDir>/tests/file-transformer.ts"
    },
    "setupFiles": [
      "./tests/setup.ts"
    ]
  }
}
